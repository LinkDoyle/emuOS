<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" stylesheets="@Terminal.css"
            xmlns="http://javafx.com/javafx/8" fx:controller="emuos.ui.TerminalController">
    <TextArea fx:id="inputArea" onKeyPressed="#handleKeyPressed" prefHeight="400.0" prefWidth="600.0" wrapText="true"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
        <font>
            <Font name="Lucida Console" size="18.0"/>
        </font>
    </TextArea>
</AnchorPane>
